size(400,400);
background(250);

int x = 50;
int y = 30;


strokeWeight(1.5);
ellipse(200,200,x+100,y+200);
ellipse(200,200,x+90,y+180);
ellipse(200,200,x+80,y+160);
ellipse(200,200,x+70,y+140);
ellipse(200,200,x+60,y+120);
ellipse(200,200,x+50,y+100);
ellipse(200,200,x+40,y+80);
ellipse(200,200,x+30,y+60);
ellipse(200,200,x+20,y+40);
ellipse(200,200,x+10,y+20);
ellipse(200,200,x,y);

save("ellipseO.jpg");
